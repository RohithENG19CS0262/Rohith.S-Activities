#include <stdio.h> 
#include <math.h> 
int input() 
{     
  int temp;     
  scanf("%d",&temp);     
  return(temp); 
    
} 
  float compute(int x1,int y1, int x2, int y2) 
{     
  float dis;     
  dis=sqrt(pow(x2-x1,2)+pow(y2-y1,2));     
  return(dis); 
} 
  void display(float d) 
{     
  printf("Distance between the points is likly %f.\n",d); 
} 
  int main() 
{     
  int x1,y1,x2,y2;     
  float distance;     
  printf("Enter the x in first point:\n");     
  x1=input();     
  printf("Enter the y in first point:\n");     
  y1=input();     
  printf("Enter the x in second point:\n");     
  x2=input();     
  printf("Enter the y in of second point:\n");     
  y2=input();     
  distance=compute(x1,y1,x2,y2);     
  display(distance);     
  return 0;
}
